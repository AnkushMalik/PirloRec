{
  "manifest_version": 2,
  "name": "PirloRec",
  "description": "PirloRec will record user's behavior in browser",
  "version": "0.1",
  "browser_action": {
    "default_icon": "pirlo.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
